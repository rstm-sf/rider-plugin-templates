<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <config>
    <!-- See https://docs.microsoft.com/en-us/nuget/consume-packages/configuring-nuget-behavior
         for config options -->
    <add key="repositorypath" value="../../../JetTestPackages" />
  </config>
  <packageSources>
    <clear />
    <!-- Add the JetBrains test package gallery first.
         Then include nuget.org to download non-test packages to include in test projects -->
    <add key="jb-gallery" value="http://jb-gallery.azurewebsites.net/api/v2/" />
    <add key="nuget.org" value="http://www.nuget.org/api/v2/" />
  </packageSources>
  <disabledPackageSources>
    <clear />
  </disabledPackageSources>
  <packageRestore>
    <!-- Allow NuGet to download missing packages -->
    <add key="enabled" value="True" />

    <!-- Automatically check for missing packages during build in Visual Studio -->
    <add key="automatic" value="False" />
  </packageRestore>
</configuration>
